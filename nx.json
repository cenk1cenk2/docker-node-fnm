{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["prebuild", "build", "lint", "lint:check", "manifest", "docs:toc"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "outputs": ["{projectRoot}/supervisor/dist"]
    },
    "manifest": {
      "outputs": ["{projectRoot}/supervisor/oclif.manifest.json"]
    },
    "docs:toc": {
      "outputs": ["{projectRoot}/supervisor/docs/**"]
    }
  },
  "defaultBase": "master"
}
